.page-rank{
    background-color: #f0f0f0;

}
.page-rank .title{
    padding-left: 10px;
    height: 55px;
    line-height: 55px;
    vertical-align: middle;
}
.page-rank .items{
    background-color: #fff;
}

.page-rank .items .item{
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: flex-start;
    align-items: center;
    border-top: 1px solid #f0f0f0;
    height: 45px;
    margin-left: auto;
}
.page-rank .items .item .nickName{
    padding-left: 10px;
    width: 200px;
}
.page-rank .items .item image{
    padding-left: 10px;
    width: 32px;
    height: 32px;
}
.page-rank .accept{
    padding-top: 10px;
    padding-bottom: 10px;
}
/* 外层组件的宽度可设置成100%或具体数值 */
.page-rank .adContainer {
    margin-top: 10px;
    width: 100%;
}