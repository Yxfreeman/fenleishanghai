.page-wrapper{
    text-align:center;
    width: 100%;
    background-color: #f8f8f8;
    padding: 10rpx;
    height: -webkit-fill-available;
}
.input-wrapper{
    width: 100%;
    text-align: center;
    margin-right: 25rpx;
}
.btn-area{
    margin: 15rpx 10rpx;
    margin-right: 25rpx;
}
input{
    height: 50px;
    line-height: 50px;
    vertical-align: middle;
    background-color: #fff;
    margin-right: 25rpx;
}
.content{
    margin-left: 10rpx;
    margin-right: 25rpx;
    background-color: #fff;
}
.content .item{
    height: 50px;
    line-height: 50px;
    vertical-align: middle;
    border-bottom: 2px solid #f9f9fe;
}
.keywords button{
    margin: 2px;
}